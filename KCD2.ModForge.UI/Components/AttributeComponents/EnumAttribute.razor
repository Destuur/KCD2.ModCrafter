@if (Attribute is not null)
{
    @if (enumNames.Any())
    {
        <MudSelect T="string" @bind-Value="CurrentEnumString" FullWidth="true" Margin="Margin.Dense" Variant="Variant.Outlined" Label="@GetTypeLabel()" Adornment="Adornment.End" AdornmentColor="Color.Secondary" AdornmentIcon="@Icons.Material.Filled.Delete" IconSize="Size.Small" OnAdornmentClick="@(() => Remove())">
            @foreach (var enumName in enumNames)
            {
                <MudSelectItem T="string" Value="@enumName">
                    @enumDisplayNames[enumName]
                </MudSelectItem>
            }
        </MudSelect>
    }

}
