<MudDialog Class="my-rounded-menu">
    <DialogContent>
        <MudText>@ContentText</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton Class="button-cancel" Style="font-size: 0.8rem !important; padding: 0.4rem 0.8rem !important;" OnClick="Cancel">Cancel</MudButton>
        <MudButton Class="button-prominent" Style="font-size: 0.8rem !important; padding: 0.4rem 0.8rem !important;" OnClick="Submit">@ButtonText</MudButton>
    </DialogActions>
</MudDialog>